@model IEnumerable<string>
@*<a asp-controller="Home" asp-action="Index" class="btn btn-light" asp-route-category=""></a>*@
<div class="container">
    <div class="row gy-1">
        @foreach (string c in Model)
        {
            <a asp-controller="Home" asp-action="Index" asp-route-category="@c" class="btn btn-primary">
                @c
            </a>
        }
    </div>
</div>

